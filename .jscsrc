  "disallowSpaceBeforePostfixUnaryOperators" : true,
  // "requireSpaceBeforePostfixUnaryOperators"  : true,

  "disallowOperatorBeforeLineBreak"          : ["."],
  "requireOperatorBeforeLineBreak"           : ["?", ":", "=", "+", "-", "/", "*", "==", "===", "!=", "!==", ">", ">=", "<", "<=", "&&", "||"],


