  "requireAlignedObjectValues"         : "ignoreLineBreak",

  // Keys
  // --------------------------------------------------------------------------
  "requireDotNotation"                 : {"allExcept": ["snake_case"]},
  "disallowQuotedKeysInObjects"        : true,
