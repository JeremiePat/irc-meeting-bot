{
  "es3"         : false,
  "esnext"      : false,
  "strict"      : true,
  "globalstrict": false,
  "evil"        : true,
  "latedef"     : true,
  "undef"       : true,
  "devel"       : false,
  "debug"       : false,
  "node"        : true,
  "browser"     : true,
  "bitwise"     : true,
